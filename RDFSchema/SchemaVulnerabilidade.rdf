<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:cve="http://cve.mitre.org/cve#"
         xmlns:grupo204="http://grupo204ws2024.org/#">

    <!-- Classe: Vulnerabilidade -->
    <rdfs:Class rdf:about="http://grupo204ws2024.org/#vulnerabilityShape">
        <rdfs:label xml:lang="en">Vulnerability</rdfs:label>
        <rdfs:label xml:lang="pt">Vulnerabilidade</rdfs:label>
        <rdfs:comment>Represents a vulnerability, including its identifier, description, publication date, and update information.</rdfs:comment>
    </rdfs:Class>

    <!-- Propriedade: ID Vulnerabilidade -->
    <rdf:Property rdf:about="http://cve.mitre.org/cve#externalIdentifier">
        <rdfs:label xml:lang="en">External Identifier</rdfs:label>
        <rdfs:label xml:lang="pt">ID Vulnerabilidade</rdfs:label>
        <rdfs:comment>Unique identifier of the vulnerability.</rdfs:comment>
        <rdfs:domain rdf:resource="http://grupo204ws2024.org/#vulnerabilityShape" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
    </rdf:Property>

    <!-- Propriedade: Título -->
    <rdf:Property rdf:about="http://cve.mitre.org/cve#title">
        <rdfs:label xml:lang="en">Title</rdfs:label>
        <rdfs:label xml:lang="pt">Título</rdfs:label>
        <rdfs:comment>Name of the vulnerability.</rdfs:comment>
        <rdfs:domain rdf:resource="http://grupo204ws2024.org/#vulnerabilityShape" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
    </rdf:Property>

    <!-- Propriedade: Descrição -->
    <rdf:Property rdf:about="http://cve.mitre.org/cve#description">
        <rdfs:label xml:lang="en">Description</rdfs:label>
        <rdfs:label xml:lang="pt">Descrição</rdfs:label>
        <rdfs:comment>Description of the vulnerability.</rdfs:comment>
        <rdfs:domain rdf:resource="http://grupo204ws2024.org/#vulnerabilityShape" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
    </rdf:Property>

    <!-- Propriedade: Data Publicação -->
    <rdf:Property rdf:about="http://cve.mitre.org/cve#published-date">
        <rdfs:label xml:lang="en">Published Date</rdfs:label>
        <rdfs:label xml:lang="pt">Data Publicação</rdfs:label>
        <rdfs:comment>Date when the vulnerability was first published.</rdfs:comment>
        <rdfs:domain rdf:resource="http://grupo204ws2024.org/#vulnerabilityShape" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
    </rdf:Property>

    <!-- Propriedade: Data Atualização -->
    <rdf:Property rdf:about="http://cve.mitre.org/cve#last-modified-date">
        <rdfs:label xml:lang="en">Last Modified Date</rdfs:label>
        <rdfs:label xml:lang="pt">Data Atualização</rdfs:label>
        <rdfs:comment>Date when the vulnerability was last updated.</rdfs:comment>
        <rdfs:domain rdf:resource="http://grupo204ws2024.org/#vulnerabilityShape" />
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string" />
    </rdf:Property>

</rdf:RDF>